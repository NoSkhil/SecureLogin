      <h1">Login</h1>
      <% include ./partials/messages %>
      <form action="/users/login" method="POST">
        <div>
          <label for="email">Email</label>
          <input
            type="email"
            id="email"
            name="email"

          />
        </div>
        <div >
          <label for="password">Password</label>
          <input
            type="password"
            id="password"
            name="password"

          />
        </div>
        <button type="submit">Login</button>
      </form>
      <p>
        No Account? <a href="/users/register">Register</a>
      </p>
    </div>
  </div>
</div>
